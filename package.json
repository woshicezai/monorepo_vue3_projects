{
  "name": "vue3-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "update-scripts": "tsx scripts/update-scripts.ts",
    "postinstall": "pnpm update-scripts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write \"**/*.{vue,ts,tsx,js,jsx,json,md}\"",
    "dev:list": "pnpm --filter list dev",
    "build:list": "pnpm --filter list build",
    "dev:vxe_table": "pnpm --filter vxe_table dev",
    "build:vxe_table": "pnpm --filter vxe_table build"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^20.x.x",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "sass": "^1.80.6",
    "terser": "^5.27.0",
    "tsx": "^4.7.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27"
  }
}
