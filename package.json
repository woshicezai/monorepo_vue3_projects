{
  "name": "vue3-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "update-scripts": "tsx scripts/update-scripts.ts",
    "postinstall": "pnpm update-scripts",
    "dev:list": "pnpm --filter pkg_list dev",
    "build:list": "pnpm --filter pkg_list build",
    "dev:vxt_table": "pnpm --filter pkg_vxt_table dev",
    "build:vxt_table": "pnpm --filter pkg_vxt_table build"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "pinia": "^2.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "typescript": "~5.3.0",
    "@changesets/cli": "^2.26.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0",
    "@types/node": "^20.x.x",
    "tsx": "^4.7.0",
    "vue-tsc": "^1.8.27",
    "terser": "^5.27.0"
  }
}
